{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "8.11.1",
    "npm": "6.1.0"
  },
  "scripts": {
    "webpack-init": "webpack-dev-server --config=webpack.dev.js",
    "precommit": "lint-staged",
    "dev": "npm-run-all --parallel dev:*",
    "dev:client": "cross-env NODE_ENV=development webpack --config webpack.dev.js --watch",
    "dev:server": "cross-env NODE_ENV=development nodemon --inspect --watch config --watch server server/server.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config=webpack.prod.js",
    "start": "cross-env NODE_ENV=production node server/server.js",
    "heroku-postbuild": "npm run build:prod",
    "test": "jest --coverage --watch",
    "test:client": "jest --config ./client/jest.config.js --coverage --watch",
    "test:server": "jest --config ./server/jest.config.js --coverage --watch",
    "test:e2e:dev": "npm-run-all --parallel dev:server cy:run",
    "test:e2e": "npm-run-all --parallel --race start cy:run",
    "cy:run": "cypress run"
  },
  "lint-staged": {
    "linters": {
      "**/*.{js,jsx,json,css,scss}": [
        "prettier --write",
        "eslint --fix",
        "git add"
      ]
    },
    "ignore": [
      ".*/dist",
      ".*/node_modules",
      "./package.json",
      "./package-lock.json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jin827/react-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jin827/react-boilerplate/issues"
  },
  "homepage": "https://github.com/Jin827/react-boilerplate#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-static-gzip": "^1.1.1",
    "history": "^4.7.2",
    "immutability-helper": "^2.7.1",
    "markdown-with-front-matter-loader": "^0.1.0",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "webpack-merge": "^4.1.4"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "brotli-webpack-plugin": "^1.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.5",
    "cypress": "^3.1.0",
    "cypress-testing-library": "^2.2.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-flow": "^2.29.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
