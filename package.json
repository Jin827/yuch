{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "8.11.1",
    "npm": "6.1.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "dev": "npm-run-all --parallel dev:*",
    "dev:client": "cross-env NODE_ENV=development webpack --config webpack.dev.js --watch",
    "dev:server": "cross-env NODE_ENV=development nodemon --inspect --watch config --watch server ./bin/www",
    "build:prod": "cross-env NODE_ENV=production webpack --config=webpack.prod.js",
    "start": "cross-env NODE_ENV=production node ./bin/www",
    "heroku-postbuild": "npm run build:prod",
    "configure": "./bin/configuredb.sh",
    "test": "cross-env NODE_ENV=test jest --coverage --watch",
    "test:client": "cross-env NODE_ENV=test jest --config ./client/jest.config.js --coverage --watch",
    "test:server": "jest --config ./server/jest.config.js --coverage --watch"
  },
  "lint-staged": {
    "linters": {
      "**/*.{js,jsx,json}": [
        "eslint --fix",
        "prettier --parser flow --write",
        "git add"
      ],
      "**/*.{scss}": [
        "prettier --parser flow --write",
        "git add"
      ]
    },
    "ignore": [
      ".*/dist",
      ".*/node_modules",
      "./package.json",
      "./package-lock.json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jin827/homemeal.git"
